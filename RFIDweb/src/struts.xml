<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="com" namespace="/" extends="struts-default">
		<action name="browse"
			class="com.myproject.action.BrowseAction" >
			<result name="fail">/Fail.jsp</result>
			<result name="success">/browse.jsp</result>		
		</action>	
		<action name="details"
			class="com.myproject.action.DetailsAction" >
			<result name="fail">/Fail.jsp</result>
			<result name="success">/browse_details.jsp</result>		
		</action>
	</package>
</struts>    
